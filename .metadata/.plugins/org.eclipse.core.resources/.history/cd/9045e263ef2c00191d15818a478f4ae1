package view;

public class Song {

		private static int idCount = 0;

		int id;
		String name;
		String artist;
		String album;
		String  year;

		Song(String name,String artist, String album, String year){

			Song.idCount++;
			this.id = idCount;
			this.name = name;
			this.artist = artist;
			this.album = album;
			this.year = year;
		}


		public int getId(){
			return this.id;
		}


}
